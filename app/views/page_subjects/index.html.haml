%h1 Listing page_subjects

%table
  %thead
    %tr
      %th Page
      %th Subject
      %th
      %th
      %th

  %tbody
    - @page_subjects.each do |page_subject|
      %tr
        %td= page_subject.page
        %td= page_subject.subject
        %td= link_to 'Show', page_subject
        %td= link_to 'Edit', edit_page_subject_path(page_subject)
        %td= link_to 'Destroy', page_subject, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Page subject', new_page_subject_path
